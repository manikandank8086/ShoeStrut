    <style>
        .labl {
            display: block;
            width: 400px;
        }

        .labl>input {
            /* HIDE RADIO */
            visibility: hidden;
            /* Makes input not-clickable */
            position: absolute;
            /* Remove input from document flow */
        }

        .labl>input+div {
            /* DIV STYLES */
            cursor: pointer;
            border: 2px solid transparent;
        }

        #couponText {
            margin-bottom: 0px;
            font-weight: 700;
            color: #046963;
            font-size: 18px
        }

        .flip-card {
            width: 250px;
            background-color: #fff;
            margin-bottom: 20px;
            height: 100px;
            border-radius: 10px;
            color: #046963;
            perspective: 1000px;
            /* Remove this if you don't want the 3D effect */
        }

        /* This container is needed to position the front and back side */
        .flip-card-inner {
            position: relative;
            width: 250px;
            height: 100px;
            text-align: center;
            transition: transform 0.8s;
            transform-style: preserve-3d;

        }

        /* Do an horizontal flip when you move the mouse over the flip box container */


        /* Position the front and back side */
        .flip-card-front,
        .flip-card-back {
            display: flex;
            position: absolute;
            width: 250px;
            height: 100px;
            -webkit-backface-visibility: hidden;
            /* Safari */
            backface-visibility: hidden;
            border: 2px solid #fff;
            border-radius: 10px;
            justify-content: center;
            align-items: center;


        }

        .labl>input:checked+div .flip-card-front {
            /* (RADIO CHECKED) DIV STYLES */
            background-color: #046963;
        }

        .labl>input:checked+div #couponText {
            color: #fff;
        }

        /* Style the front side (fallback if image is missing) */
        .flip-card-front {
            background-color: #fff;
            color: #046963;
            border: 2px solid #046963;
        }

        /* Style the back side */
        .flip-card-back {
            height: 100px;
            width: 250px;
            background-color: #fff;
            color: #046963;
            transform: rotateY(180deg);
        }
    </style>

</head>


            document.getElementById('addressError').innerHTML = address ? "" : "Please enter your address";
            document.getElementById('cityError').innerHTML = city ? (city.match(stateCityRegex) ? "" : "City must contain only letters") : "Please enter your city/district/town";
            document.getElementById('stateError').innerHTML = state ? (state.match(stateCityRegex) ? "" : "State must contain only letters") : "Please enter your state"; // Show error message if state is empty or not containing only letters
            document.getElementById('landmarkError').innerHTML = landmark ? (landmark.match(stateCityRegex) ? "" : "Landmark must contain only letters") : "Please enter a landmark";
            isValid = false;
        } else {
            // All required fields are filled
            document.getElementById('nameError').innerHTML = "";
            document.getElementById('phoneError').innerHTML = "";
            document.getElementById('addressTypeError').innerHTML = "";
            document.getElementById('addressError').innerHTML = "";
            document.getElementById('cityError').innerHTML = "";
            document.getElementById('stateError').innerHTML = "";
            document.getElementById('landmarkError').innerHTML = "";
        }

        // Validate other fields
        if (pincode && !pincodeRegex.test(pincode)) {
            document.getElementById('pincodeError').innerHTML = "Please enter a valid 6 digit pincode";
            isValid = false;
        } else {
            document.getElementById('pincodeError').innerHTML = "";
        }

        if (alternatePhone && !phoneRegex.test(alternatePhone)) {
            document.getElementById('alternatePhoneError').innerHTML = "Please enter a valid 10 digit alternate phone number";
            isValid = false;
        } else {
            document.getElementById('alternatePhoneError').innerHTML = "";
        }

        return isValid;
    }
</script>

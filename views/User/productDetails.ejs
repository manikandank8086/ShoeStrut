    let rating1 = '<%= userRated %>';
    console.log('hello ', rating1);
    document.querySelector('input[name="rating"][value="' + rating1 + '"]').checked = true;





    async function Star(rate) {
        const productId = `<%= product._id %>`

        console.log(productId)
        console.log(rate)
        try {
            const response = await fetch('/product/star', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ rate: rate, productId: productId })
            })

            const data = await response.json()
            if (data.status) {

                showToast('Your rating has been saved ', "green");
            } else {
                showToast('Error in rating', "red");
            }
        } catch (error) {

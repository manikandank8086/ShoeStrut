        }
    }



    //add to Cart
    async function addToCart(id) {

        console.log(id);
        Swal.fire({
            title: 'Item added',
            icon: 'success',
            showCancelButton: true,
            confirmButtonText: 'OK',
            cancelButtonText: 'Cancel'
        }).then(async (result) => {
            if (result.isConfirmed) {
                try {
                    const response = await fetch('/product/addcart/' + id, {
                        method: 'GET'
                    });
                    const data = await response.json();
                    console.log(response);
                    if (response.status == 200) {
                        location.reload();
                    } else {
                        console.error(data.message);
                    }
                } catch (error) {
                    console.error('Fetch Error:', error);
                }
            }
        });
    }



    function WhislistAdd(id) {

        fetch('/product/addWhislis/<%= product._id %>', {
            method: 'GET',
            headers: {
                'Content-Type': 'application/json'
            },
        }).then(async (response) => {
            if (response.ok) {
                const data = await response.json()
                if (data.status == 'included') {
                    Swal.fire({
                        title: 'Item Already Added',
                        text: 'This item is already in your wishlist.',
                        icon: 'warning',
                        confirmButtonText: 'OK',
                        customClass: {
                            icon: 'icon-class',
                        },
                    });
                } else {
                    Swal.fire({
                        title: 'Item Added Successfully',
                        text: 'This item has been added to your wishlist.',
                        icon: 'success',
                        confirmButtonText: 'OK',
                        customClass: {
                            icon: 'icon-class',
                        },
                    });
                }

            }
        }).catch((error) => {
            console.log(error)
        })
    }





    function quantityDown(id) {
        alert("dec")
        updateQuantity('decrement', `${id}`)

        console.log(id)
    }

    function quantityUp(id) {
        alert("inc")
        updateQuantity('increment', `${id}`)
        console.log(id)

    }

    function updateQuantity(action, productId) {
        alert('fine')
        const quantityElement = document.getElementById(`quantityValue_${productId}`);
        const currentQuantity = parseInt(quantityElement.innerText);


        fetch(`/product/updateQuantity/${productId}`, {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
            },
            body: JSON.stringify({
                action: action,
                currentQuantity: currentQuantity,
            }),
        })
            .then(response => response.json())
            .then(data => {

                console.log("quantity : ", data)
                quantityElement.innerText = data.quantity;
                document.getElementById(`${data._id}`).innerText = data.total
            })
            .catch(error => {
                console.log('Fetch Error:', error);
            });
    }




